{
  "compilerOptions": {
    "typeRoots" : ["src/typesmodules/@types", "types"],
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
  },
  "include": [
    "src/*",
    "src/maps/*",
    "src/types/asset.d.ts",
    "src/types/jpg.d.ts",
    "src/types/png.d.ts"
  ]
}